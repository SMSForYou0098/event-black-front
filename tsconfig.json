{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": ["./typings", "./node_modules/@types"],
    "types": ["jquery", "datatables.net"],
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/form/form-validation.tsx",
    "src/pages/special-pages/billing.tsx",
    "src/components/DataTable.tsx",
    "src/pages/design-system/typography.tsx",
    "src/components/custom.d.ts",
    ".next/types/**/*.ts",
    "src/pages/index.jsx",
    "src/components/cards/CardStyle.jsx",
    "src/pages/auth/verify-password.jsx",
    "src/pages/auth/lost-password.jsx",
    "src/pages/auth/sign-up.jsx",
    "src/pages/auth/login.jsx",
    "src/pages/auth/auth-success.jsx",
    "src/pages/auth/two-factor.jsx",
    "src/lib/axiosInterceptor.js",
    "src/pages/_app.jsx"
, "src/layouts/Frontend.jsx", "src/store/auth/authSlice.js"  ],
  "exclude": ["node_modules"]
}
